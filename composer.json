{
    "name": "yusam-hub/php-compatibility",
    "description": "yusam-hub/php-compatibility",
    "type": "package",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
